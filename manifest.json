{
  "name": "Cereal Com",
  "description": "Connect and communicate through your computer's serial ports",
  "version": "1.1.5",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },
  "permissions": [
    "serial",
    "storage",
    {"fileSystem": ["write", "retainEntries", "directory"]} ],
  "icons": { "16": "icon-16.png", "48": "icon-48.png", "64": "icon-64.png", "128": "icon-128.png" },
  "offline_enabled": true
}